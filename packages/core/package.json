{
  "name": "@mtfe/nami-ui",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/nami-ui/nami-ui.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "npm run clean && stencil build --docs --es5 --docs-json dist/docs.json",
    "clean": "node scripts/clean.js",
    "cdnloader": "node scripts/copy-cdn-loader.js",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.12.0",
    "@stencil/react-output-target": "^0.2.0",
    "@stencil/vue-output-target": "^0.6.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "fs-extra": "^10.0.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3"
  },
  "license": "MIT"
}
